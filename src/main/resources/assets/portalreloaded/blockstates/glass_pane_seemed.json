{
    "multipart": [
        {   "when": { "facing": "north" },
            "apply": { "model": "portalreloaded:glass_pane_seemed" }
        },
        {   "when": { "facing": "east" },
            "apply": { "model": "portalreloaded:glass_pane_seemed", "y": 90 }
        },
        {   "when": { "facing": "south" },
            "apply": { "model": "portalreloaded:glass_pane_seemed", "y": 180 }
        },
        {   "when": { "facing": "west" },
            "apply": { "model": "portalreloaded:glass_pane_seemed", "y": 270 }
        },
        {   "when": { "facing": "down" },
            "apply": { "model": "portalreloaded:glass_pane_seemed", "x": 90 }
        },
        {   "when": { "facing": "up" },
            "apply": { "model": "portalreloaded:glass_pane_seemed", "x": 270 }
        }
    ]
}
